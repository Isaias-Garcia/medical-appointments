generator client {
  provider = "prisma-client-js"
  output   = "./client"
}

datasource dbPE {
  provider = "postgresql"
  url      = env("DATABASE_PE_URL")
}

model AppointmentPE {
  id         Int      @id @default(autoincrement())
  insuredId  String
  scheduleId Int
  countryISO String
  status     String
  createdAt  DateTime @default(now())
}
